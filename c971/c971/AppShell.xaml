<?xml version="1.0" encoding="UTF-8" ?>
<Shell xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
       xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
       xmlns:pages="clr-namespace:c971.Pages"
       x:Class="c971.AppShell">

    <FlyoutItem Title="Home">
        <Tab>
            <ShellContent Title="Home" ContentTemplate="{DataTemplate pages:MainPage}" />
        </Tab>
    </FlyoutItem>

    <FlyoutItem Title="Reports">
        <Tab>
            <ShellContent Title="Reports" ContentTemplate="{DataTemplate pages:ReportsPage}" />
        </Tab>
    </FlyoutItem>

    <FlyoutItem Title="Sign Up">
        <Tab>
            <ShellContent Title="Sign Up" ContentTemplate="{DataTemplate pages:SignUpPage}" />
        </Tab>
    </FlyoutItem>
</Shell>
